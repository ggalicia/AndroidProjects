    <!-- header text for the editText input [CHAR LIMIT=20] -->
    <string name="name">Name</string>

    <!-- header text for the list of toppings [CHAR LIMIT=20] -->
    <string name="toppings">Toppings</string>

    <!-- text for the Whipped Cream option [CHAR LIMIT=20] -->
    <string name="whipped_cream">Whipped Cream</string>

    <!-- text for the Chocolate option [CHAR LIMIT=20] -->
    <string name="chocolate">Chocolate</string>

    <!-- header text for  Quantity  [CHAR LIMIT=20] -->
    <string name="quantity">Quantity</string>

    <!-- header text for the Order Summary  [CHAR LIMIT=20] -->
    <string name="order_summary">Order Summary</string>

    <!-- text for the Order Button  [CHAR LIMIT=20] -->
    <string name="order_button">Order</string>

    <!-- text for the customer name. Do not translate customer name.  [CHAR LIMIT=None] -->
    <string name="customer_name">Name: <xliff:g id="name" example="Amy">%s</xliff:g></string>

