<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.sdpvocabquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".DashboardActivity"
            android:label="Dashboard" />
        <activity
            android:name=".ViewQuizNames"
            android:label="ViewStats" />
        <activity
            android:name=".NewQuizActivity"
            android:label="NewQuiz" />
        <activity
            android:name=".NewWordActivity"
            android:exported="true"
            android:label="NewWord" />
        <activity
            android:name=".QuestionActivity"
            android:label="Question" />
        <activity
            android:name=".QuizResultActivity"
            android:label="QuizResult" />
        <activity android:name=".ViewQuizStatsActivity"></activity>
    </application>

</manifest>